_code = {
"fragment": "float ha( float n ) {return fract(sin(n)*758);}

float no( vec3 x ) {
	vec3 p = floor(x);
	vec3 f = fract(x); 
	float n = p.x + p.y*57.0 + p.z*800.0;
	float res = mix(mix(mix( ha(n+  0.0), ha(n+  1.0),f.x), mix( ha(n+ 57.0), ha(n+ 58.0),f.x),f.y),
	mix(mix( ha(n+800.0), ha(n+801.0),f.x), mix( ha(n+857.0), ha(n+858.0),f.x),f.y),f.z);
	return res;}

float fbm( vec3 p ) {
	float f = 0.3*cos(TIME*0.03);
	f += 0.50000*no( p );
	p = p*2.02;
	f -= 0.25000*no( p );
	p = p*2.03;
	f += 0.12500*no( p );
	p = p*2.01;
	f += 0.06250*no( p );
	p = p*2.04;
	f -= 0.03125*no( p );
	return f/0.984375;}

float cloud(vec3 p) {
	p-=fbm(vec3(p.x,p.y,0.0)*0.27)*2.27;
	float a = 0.0;
	a-=fbm(p*3.0)*2.2-1.1;
	if (a<0.0) { a = 0.0; a = a * a; }
	return a; }

vec2 position = vec2(1) - UV;
position.y+=0.2;
vec2 coord= vec2((position.x-0.5)/position.y,1.0/(position.y-0.3));
coord += TIME*0.1+1000.0;
float q = cloud(vec3(coord*1.0,0.222));
vec3 col = vec3(0.3,0.3,0.5) + vec3(0.5*q,0.5*q,0.5*q);
COLOR = vec4( col, 0.5);",
